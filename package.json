{
  "name": "recreator",
  "version": "0.1.0-beta.5",
  "description": "Object factory with polyconstructors and post reassignment",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "scripts": {
    "test": "eslint src && ./node_modules/.bin/jest",
    "build": "npm run build:es && npm run build:commonjs",
    "build:es": "cross-env BABEL_ENV=es babel src -d es",
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src -d lib",
    "prepublish": "npm run clean && npm run build",
    "clean": "rimraf es lib",
    "manual": "node manualTester.js"
  },
  "keywords": [],
